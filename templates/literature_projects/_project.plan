| block title self.title end |

| block content |
	<p>|self.type|, |self.year|</p>
	| ifdef self.quote |
		<blockquote>
		| for p in self.quote |
			<p>|for s in p s " " end|</p>
		| end |
		</blockquote>
	| end |
	| for p in self.description |
		<p>|for s in p s " " end|</p>
	| end |
	| ifdef self.links |
		<h2>Linkkej√§</h2>
		<ul>
			| for link in self.links |
				<li>|link.title|: <a href="|link.url|">|link.url|</a>
			| end |
		</ul>
	| end |
| end |

| include "templates/_page.plan" |
